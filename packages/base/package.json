{
  "name": "@35up/js-sdk-base",
  "version": "0.5.1",
  "description": "Javascript SDK library for integrating 35up into your web shop",
  "scripts": {
    "build": "ENV=production rollup --config rollup.config.js",
    "build:stage": "ENV=staging rollup --config rollup.config.js",
    "build:mock": "ENV=mock rollup --config rollup.config.js",
    "lint": "eslint --ext .js,.ts .",
    "prepublishOnly": "make ci",
    "test": "jest --passWithNoTests",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "makeTypeDeclarations": "tsc -d --declarationDir dist/types --emitDeclarationOnly index.ts",
    "bump-version": "lerna version --no-git-tag-version --no-push",
    "bootstrap": "lerna bootstrap"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/35up/js-sdk.git"
  },
  "keywords": [
    "bootstrap"
  ],
  "author": "35up GmbH <the.engineers@35up.com>",
  "license": "UNLICENCED",
  "bugs": {
    "url": "https://github.com/35up/js-sdk/issues"
  },
  "homepage": "https://github.com/35up/js-sdk#readme",
  "dependencies": {
    "@35up/http-client": "^2.1.0",
    "@35up/tslib-utils": "^2.0.1",
    "nanoid": "^3.3.4",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@35up/eslint-config-35up-base": "^4.0.2",
    "@35up/tslib-test-utils": "^1.0.1",
    "@babel/core": "^7.18.13",
    "@babel/preset-env": "^7.18.10",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-typescript": "^8.4.0",
    "@types/chai": "^4.3.3",
    "@types/chai-string": "^1.4.2",
    "@types/jest": "^28.0.0",
    "@types/sinon": "^10.0.13",
    "@types/sinon-chai": "^3.2.8",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "babel-jest": "^28.1.0",
    "chai": "^4.3.6",
    "chai-string": "^1.5.0",
    "eslint": "^8.23.0",
    "jest": "^28.1.0",
    "jest-environment-jsdom": "^28.1.1",
    "jest-fetch-mock": "^3.0.3",
    "lerna": "^5.5.2",
    "nx": "^14.7.15",
    "rollup": "^2.79.0",
    "rollup-plugin-terser": "^7.0.2",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.2"
  },
  "iife": "dist/35up-js-sdk.iife.min.js",
  "main": "dist/35up-js-sdk.cjs.js",
  "amd": "dist/35up-js-sdk.amd.min.js",
  "module": "dist/35up-js-sdk.esm.js",
  "types": "dist/types",
  "workspaces": [
    "packages/*"
  ]
}
