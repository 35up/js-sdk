{
  "name": "@35up/js-sdk",
  "version": "0.3.0",
  "description": "Javascript SDK library for integrating 35up into your web shop",
  "scripts": {
    "build": "ENV=production rollup --config rollup.config.js",
    "build:stage": "ENV=staging rollup --config rollup.config.js",
    "build:mock": "ENV=mock rollup --config rollup.config.js",
    "lint": "eslint --ext .js,.ts .",
    "prepublishOnly": "make ci",
    "test": "jest --passWithNoTests",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "makeTypeDeclarations": "tsc -d --declarationDir dist/types --emitDeclarationOnly src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/35up/js-sdk.git"
  },
  "keywords": [
    "bootstrap"
  ],
  "author": "35up GmbH <the.engineers@35up.com>",
  "license": "UNLICENCED",
  "bugs": {
    "url": "https://github.com/35up/js-sdk/issues"
  },
  "homepage": "https://github.com/35up/js-sdk#readme",
  "dependencies": {
    "@35up/http-client": "^1.1.3",
    "@35up/tslib-frontend-utils": "^1.0.5",
    "nanoid": "^3.3.4",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@babel/preset-env": "^7.18.2",
    "@35up/eslint-config-35up-base": "^3.0.0",
    "@rollup/plugin-commonjs": "^22.0.0",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-typescript": "^8.3.2",
    "@types/chai": "^4.3.1",
    "@types/chai-string": "^1.4.2",
    "@types/jest": "^28.1.0",
    "@types/sinon": "^10.0.11",
    "@types/sinon-chai": "^3.2.8",
    "babel-jest": "^28.1.0",
    "chai": "^4.3.6",
    "chai-string": "^1.5.0",
    "dotenv": "^16.0.1",
    "eslint": "^7.32.0",
    "jest": "^28.1.0",
    "jest-fetch-mock": "^3.0.3",
    "rollup": "^2.75.5",
    "rollup-plugin-terser": "^7.0.2",
    "sinon": "^14.0.0",
    "sinon-chai": "^3.7.0",
    "ts-jest": "^28.0.4",
    "typescript": "^4.7.2"
  },
  "iife": "dist/35up-js-sdk.iife.min.js",
  "main": "dist/35up-js-sdk.cjs.js",
  "amd": "dist/35up-js-sdk.amd.min.js",
  "module": "dist/35up-js-sdk.esm.js",
  "types": "dist/types"
}
