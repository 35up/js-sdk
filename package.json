{
  "name": "@caseable/js-sdk",
  "version": "0.1.0",
  "description": "Javascript SDK library for integrating 35up into your web shop",
  "scripts": {
    "build": "ENV=production rollup --config rollup.config.js",
    "build:stage": "ENV=staging rollup --config rollup.config.js",
    "build:mock": "ENV=mock rollup --config rollup.config.js",
    "lint": "eslint --ext .js,.ts .",
    "prepublishOnly": "make ci",
    "test": "jest --passWithNoTests",
    "test:watch": "npm test -- --watch",
    "test:coverage": "npm test -- --coverage",
    "makeTypeDeclarations": "tsc -d --declarationDir dist/types --emitDeclarationOnly"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/caseable/js-sdk.git"
  },
  "keywords": [
    "bootstrap"
  ],
  "author": "35up GmbH <it@35up.com>",
  "license": "UNLICENCED",
  "bugs": {
    "url": "https://github.com/caseable/js-sdk/issues"
  },
  "homepage": "https://github.com/caseable/js-sdk#readme",
  "dependencies": {
    "@caseable/http-client": "0.0.12",
    "@caseable/tslib-frontend-utils": "0.0.6",
    "nanoid": "^3.1.20",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@caseable/eslint-config-35up-base": "0.0.4",
    "@rollup/plugin-commonjs": "^16.0.0",
    "@rollup/plugin-node-resolve": "^10.0.0",
    "@rollup/plugin-replace": "^2.3.2",
    "@rollup/plugin-typescript": "^6.1.0",
    "@types/chai": "^4.2.0",
    "@types/chai-string": "^1.4.2",
    "@types/jest": "^26.0.9",
    "@types/sinon": "^9.0.8",
    "@types/sinon-chai": "^3.2.5",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "chai": "^4.2.0",
    "chai-string": "^1.4.2",
    "dotenv": "^8.2.0",
    "eslint": "^7.5.0",
    "jest": "^26.6.3",
    "jest-fetch-mock": "^3.0.3",
    "rollup": "^2.22.1",
    "rollup-plugin-terser": "^7.0.2",
    "sinon": "^9.0.8",
    "sinon-chai": "^3.2.5",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5"
  },
  "iife": "dist/35up-js-sdk.iife.min.js",
  "main": "dist/35up-js-sdk.cjs.js",
  "amd": "dist/35up-js-sdk.amd.min.js",
  "module": "dist/35up-js-sdk.esm.js",
  "types": "dist/types"
}
